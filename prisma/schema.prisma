datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int    @default(autoincrement()) @id
  username String @unique
  books    Book[]
}

model Book {
  id      Int    @default(autoincrement()) @id
  user    User   @relation(fields: [userId], references: [id])
  userId  Int
  title   String
  content String
}